<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidbootcamp.mycruiseapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Disney_Dream" />
        <activity
            android:name=".DisneyDream_Price"
            android:label="@string/title_activity_disney_dream__price"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DisneyDream_activities" />
        <activity android:name=".Activities_list" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".DisneyDream_Guest" />
        <activity android:name=".EmpActivity" />
        <activity android:name=".Portsofcall" />
        <activity android:name=".Thingstosee3nights" />
        <activity android:name=".Thingstosee5nights" />
        <activity android:name=".Thingstosee7nights" />
        <activity android:name=".ActivitiesCastaway" />
        <activity android:name=".ActivitiesNassau" />
        <activity android:name=".ActivitiesCozumel" />
        <activity android:name=".ActivitiesGeorge" />
        <activity android:name=".BuyActivities" />
        <activity android:name=".RoomService"></activity>
    </application>

</manifest>